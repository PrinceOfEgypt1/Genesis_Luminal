# Snyk (https://snyk.io) policy file
version: v1.25.0
ignore: {}

# Patches applied by Snyk
patch: {}

# Language settings
language-settings:
  javascript:
    packageManager: npm
    
# Exclude paths
exclude:
  global:
    - node_modules/
    - dist/
    - build/
    - coverage/
    - '**/*.test.ts'
    - '**/*.test.js'
    - '**/*.spec.ts'
    - '**/*.spec.js'
